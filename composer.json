{
    "name": "okpt/app-project",
    "description": "Project bundle skeleton",
    "keywords": ["okpt","importer"],
    "type": "project",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Kevin Piam",
            "email": "kevinpiam3@yahoo.com"
        }
    ],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-memcached": "*",
        "ext-openssl": "*",
        "api-platform/core": "^2.7",
        "beberlei/doctrineextensions": "^1.3",
        "composer/package-versions-deprecated": "1.11.99.1",
        "cweagans/composer-patches": "^1.7",
        "doctrine/annotations": "^2.0",
        "doctrine/collections": "^2.2",
        "doctrine/dbal": "^3.3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "dompdf/dompdf": "^2.",
        "easycorp/easyadmin-bundle": "^4.",
        "endroid/qr-code-bundle": "^4.",
        "friendsofsymfony/elastica-bundle": "^6.",
        "guzzlehttp/guzzle": "^7.9",
        "jms/serializer-bundle": "^5.",
        "keyvanakbary/slugifier": "^4.",
        "knplabs/doctrine-behaviors": "^2.",
        "myclabs/deep-copy": "^1.11",
        "nelmio/cors-bundle": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "^6.0.",
        "symfony/filesystem": "^6.0.",
        "symfony/flex": "^2",
        "symfony/form": "^6.0.",
        "symfony/framework-bundle": "6.4.*",
        "symfony/google-mailer": "6.0.*",
        "symfony/http-client": "^6.0.",
        "symfony/http-foundation": "^6.0.",
        "symfony/http-kernel": "6.4.*",
        "symfony/intl": "^6.0.",
        "symfony/lock": "^6.0.",
        "symfony/mailer": "^6.0.",
        "symfony/mailgun-mailer": "6.0.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/process": "^6.0.",
        "symfony/property-access": "^6.0.",
        "symfony/property-info": "^6.0.",
        "symfony/proxy-manager-bridge": "^6.0.",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "^6.0.",
        "symfony/sendgrid-mailer": "6.4.*",
        "symfony/serializer": "^6.0.",
        "symfony/templating": "^6.0.",
        "symfony/translation": "^6.0.",
        "symfony/twig-bundle": "^6.0.",
        "symfony/uid": "^6.1.",
        "symfony/ux-dropzone": "^2.17",
        "symfony/validator": "^6.0.",
        "symfony/web-link": "^6.0.",
        "symfony/workflow": "6.4.*",
        "symfony/yaml": "6.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2."
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "psr/container": "^2.0",
        "slevomat/coding-standard": "^8.6",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/dom-crawler": "6.4.*",
        "symfony/maker-bundle": "^1.57",
        "symfony/var-dumper": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "endroid/installer": true,
            "cweagans/composer-patches": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-domains": ["github.com"],
        "secure-http": false,
        "github-protocols": ["https"],
        "http-basic": {
            "repo.packagist.org": {
                "username": "YourUserName",
                "password": "YourPassword"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "okpt\\app\\project\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check": "vendor/bin/phpcs src/",
        "fix": "vendor/bin/phpcbf src/"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
